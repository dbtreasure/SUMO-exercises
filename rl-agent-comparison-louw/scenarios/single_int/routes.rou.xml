<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <!-- Vehicle type -->
    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5" maxSpeed="13.89"/>

    <!-- Routes: straight-through movements -->
    <route id="route_N_S" edges="N_to_C C_to_S"/>
    <route id="route_S_N" edges="S_to_C C_to_N"/>
    <route id="route_E_W" edges="E_to_C C_to_W"/>
    <route id="route_W_E" edges="W_to_C C_to_E"/>

    <!-- Flows: 0-3600s, moderate volume (~360 veh/hr per direction = 0.1 veh/s) -->
    <flow id="flow_N_S" type="car" route="route_N_S" begin="0" end="3600" vehsPerHour="360"/>
    <flow id="flow_S_N" type="car" route="route_S_N" begin="0" end="3600" vehsPerHour="360"/>
    <flow id="flow_E_W" type="car" route="route_E_W" begin="0" end="3600" vehsPerHour="360"/>
    <flow id="flow_W_E" type="car" route="route_W_E" begin="0" end="3600" vehsPerHour="360"/>
</routes>
